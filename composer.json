{
    "name": "presta/cms-social-bundle",
    "type": "symfony-bundle",
    "description": "PrestaCMS Social bundle : Add social block to PrestaCMS",
    "keywords": ["symfony2", "bundle", "cms", "symfony-cmf", "prestaconcept"],
    "homepage": "https://github.com/prestaconcept/PrestaCMSSocialBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alain Flaus",
            "email": "aflaus@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "guzzle/guzzle": "3.8.*"
    },
    "suggest": {
        "presta/cms-core-bundle": "1.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Presta\\CMSSocialBundle": "" }
    },
    "target-dir": "Presta/CMSSocialBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
